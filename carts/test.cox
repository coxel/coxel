test("Array", function() {
  let arr = [1, 2, 3];
  check(arr[-1], undefined);
  check(arr[0], 1);
  check(arr[1], 2);
  check(arr[2], 3);
  check(arr[3], undefined);
  check(arr.length, 3);
  arr.push("4");
  check(arr[-1], undefined);
  check(arr[0], 1);
  check(arr[1], 2);
  check(arr[2], 3);
  check(arr[3], "4");
  check(arr[4], undefined);
  check(arr.length, 4);
  check(arr.pop(), "4");
