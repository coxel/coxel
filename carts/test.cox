  };
  check(f(), undefined);
  check(f(1), 1);
  check(f(1, 2), 1);
  check(f(1, 2, 3), 1);
  check(g(), undefined);
  check(g(1), undefined);
  check(g(1, 2), 2);
  check(g(1, 2, 3), 2);
});

