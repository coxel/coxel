  check(arr.length, 3);
});

test("Object", function() {
  let obj = { a: "a", "b": "b", "10": "c" };
  check(obj.a, "a");
  check(obj["a"], "a");
  check(obj.b, "b");
  check(obj["b"], "b");
  check(obj.c, undefined);
  check(obj["c"], undefined);
  check(obj[10], "c");

  check("a" in obj, true);
  obj.a = undefined;
  check(obj.a, undefined);
  check("a" in obj, true);

  obj.d = "t";
  check(obj.d, "t");
  obj.e = obj;
  check(obj.e, obj);
  check(obj.e.e.e, obj);
});

test("Recursive", function() {
  let frac = function f(n) {
    if (n == 0)
      return 1;
    else
      return n * f(n - 1);
  };
  check(frac(0), 1);
  check(frac(1), 1);
  check(frac(2), 2);
  check(frac(3), 6);
  check(frac(4), 24);
  check(frac(5), 120);
  check(frac(6), 720);
  check(frac(7), 5040);

  let fib = function f(n) {
    if (n == 0)
      return 0;
    else if (n == 1)
      return 1;
    else
      return f(n - 1) + f(n - 2);
  };
  check(fib(0), 0);
  check(fib(1), 1);
  check(fib(2), 1);
  check(fib(3), 2);
  check(fib(4), 3);
  check(fib(5), 5);
  check(fib(6), 8);
  check(fib(7), 13);
  check(fib(8), 21);
  check(fib(9), 34);
  check(fib(10), 55);
  check(fib(11), 89);
  check(fib(12), 144);
});

