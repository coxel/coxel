});

test("Live-range", function() {
  let a = 10;
  check(a, 10);
  {
    let a = 20, b = 10, c = 30;
    check(a, 20);
    a = 30;
    check(a, 30);
    check(b, 10);
    b = "a";
    check(b, "a");
    check(c, 30);
    c = "d";
    check(c, "d");
  }
  let c = "q";
  check(a, 10);
  check(b, undefined);
  check(c, "q");
});

test("Upvalue", function() {
  let t;
  let a = "a";
  {
    let a = 10;
    let f = function() {
      let g = function() {
        return a;
      };
      return g;
    };
    t = f();
    check(t(), 10);
    a = 20;
    check(t(), 20);
  }
  check(t(), 20);
});

